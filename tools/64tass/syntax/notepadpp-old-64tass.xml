<NotepadPlus>
    <UserLang name="tass64" ext="a asm">
        <Settings>
            <Global caseIgnored="yes" />
            <TreatAsSymbol comment="no" commentLine="yes" />
            <Prefix words1="no" words2="no" words3="no" words4="yes" />
        </Settings>
        <KeywordLists>
            <Keywords name="Delimiters">&quot;&apos;0&quot;&apos;0</Keywords>
            <Keywords name="Folder+">.proc .if .ifeq .ifne .ifpl .ifmi .switch .segment .macro .section .function .struct .union</Keywords>
            <Keywords name="Folder-">.pend .fi .endif .endswitch .endm .send .endf .ends .endu</Keywords>
            <Keywords name="Operators">- ! # &amp; ( ) * , / : ? [ ] { } ^ ` | ~ + &lt; = &gt;</Keywords>
            <Keywords name="Comment"> 1 1.comment 2.endc 0;</Keywords>
            <Keywords name="Words1">adc and asl bcc bcs beq bge bit blt bmi bne bpl brk bvc bvs clc cld cli clv cmp cpx cpy dec dex dey eor gcc gcs geq gge glt gmi gne gpl gvc gvs inc inx iny jmp jsr lda ldx ldy lsr nop ora pha php pla plp rol ror rti rts sbc sec sed sei shl shr sta stx sty tax tay tsx txa txs tya ahx alr anc ane arr asr axs dcm dcp ins isb isc jam lae las lax lds lxa rla rra sax sbx sha shs shx shy slo sre tas xaa</Keywords>
            <Keywords name="Words2">.al .align .as .assert .bend .binclude .block .break .case .cdef .cerror .check .continue .cpu .cwarn .databank .default .dpage .dsection .dstruct .dunion .edef .else .elsif .enc .end .endp .ends .endswitch .endu .endweak .eor .error .for .goto .here .hidemac .include .lbl .logical .next .offs .option .page .proff .pron .rept .showmac .switch .var .warn .weak .xl .xs .autsiz .mansiz .seed</Keywords>
            <Keywords name="Words3">.byte .binary .char .ptext .lint .long .null .dword .sint .fill .rta .shift .shiftl .dint .word .text .addr</Keywords>
            <Keywords name="Words4">$ %</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="11" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="FOLDEROPEN" styleID="12" fgColor="008000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="FOLDERCLOSE" styleID="13" fgColor="008000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD1" styleID="5" fgColor="0000A0" bgColor="FFFFFF" fontName="" fontStyle="1" />
            <WordsStyle name="KEYWORD2" styleID="6" fgColor="008000" bgColor="FFFFFF" fontName="" fontStyle="1" />
            <WordsStyle name="KEYWORD3" styleID="7" fgColor="FF8000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="KEYWORD4" styleID="8" fgColor="FF0000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="COMMENT" styleID="1" fgColor="804000" bgColor="FFFFFF" fontName="" fontStyle="2" />
            <WordsStyle name="COMMENT LINE" styleID="2" fgColor="804000" bgColor="FFFFFF" fontName="" fontStyle="2" fontSize="10" />
            <WordsStyle name="NUMBER" styleID="4" fgColor="FF0000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="OPERATOR" styleID="10" fgColor="400040" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER1" styleID="14" fgColor="FF8000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER2" styleID="15" fgColor="FF8000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER3" styleID="16" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
